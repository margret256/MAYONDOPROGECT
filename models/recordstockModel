const mongoose = require("mongoose");

const recordstockSchema = new mongoose.Schema({
productName: {
    type: String,
    // required: true,
  },
  productType: {
    type: String,
    // required: true,
  },
  costPrice: {
    type: Number,
    // required: true,
  },
  salePrice: {
    type: Number,
    // required: true,
  },
  quantity: {
    type: Number,
    // required: true,
  }
});

module.exports = mongoose.model("recordstockModel", recordstockSchema);
